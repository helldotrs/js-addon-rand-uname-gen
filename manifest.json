{
  "manifest_version": 2,
  "name": "Username Generator",
  "version": "1.0",
  "description": "Generate usernames for text input boxes.",
  "icons": {
    "48": "icon.png"
  },
  "permissions": ["contextMenus"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
